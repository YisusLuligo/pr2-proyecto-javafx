<HBox alignment="CENTER_LEFT" prefHeight="46.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.cliente.marketplaceapp.controller.PersonaController">
